wflags = -Wall -Wextra -Wno-missing-field-initializers -Wno-unused-parameter
oflags = -Ofast -Os -DNDEBUG -fno-common -dynamic
cflags = $wflags $oflags -DCONFIG_FILE='"conf/osx.h"' -I. -Imacosx
ldflags = -g $oflags
libs = -framework Foundation
rule cc
  command = cc $cflags -c $in -o $out
rule ld
  command = cc $ldflags $in $libs -o $out
build alloc.o: cc alloc.c
build aplfuncs.o: cc aplfuncs.c
build archive.o: cc archive.c
build arith.o: cc arith.c
build array.o: cc array.c
build buf.o: cc buf.c
build call.o: cc call.c
build catch.o: cc catch.c
build cfunc.o: cc cfunc.c
build cfunco.o: cc cfunco.c
build compile.o: cc compile.c
build conf.o: cc conf.c
build control.o: cc control.c
build crc.o: cc crc.c
build crc32c.o: cc crc32c.c
build error.o: cc error.c
build events.o: cc events.c
build exec.o: cc exec.c
build exerror.o: cc exerror.c
build file.o: cc file.c
build findpath.o: cc findpath.c
build float.o: cc float.c
