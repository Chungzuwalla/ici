file = fopen("test-save-float.ici.dat");
o = restore(file);
close(file);
if (typeof(o) != "float")
    fail("restore failed: restored a " + typeof(o));
if (o != 1.0) {
    fail(sprintf("%f: expected to restore 1.0 and we didn't", o));
}
exit();
