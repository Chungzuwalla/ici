local fname = tmpname();

local
count1000()
{
    j = 0;
    for (i = 0; i < 1000; ++i)
        j += i;
}

local
count2000()
{
    count1000();
    count1000();
}

local
prof()
{
    profile(fname);
    count1000();
    count2000();
}

if (version() ~ #prof#)
{
    prof();

    include(fname);
    remove(fname);
    if (typeof(profile.total) != "int")
	fail("profile total absent");

    if (typeof(profile.calls) != "struct")
	fail("profile sub-calls absent");
}
